<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddGoalsController">
   <children>
      <Label contentDisplay="CENTER" layoutX="117.0" layoutY="75.0" prefHeight="63.0" prefWidth="267.0" text="Add Goals" textAlignment="RIGHT">
         <font>
            <Font name="Cambria Bold" size="32.0" />
         </font>
      </Label>
      <HBox prefHeight="63.0" prefWidth="500.0" style="-fx-background-color: #12C1E0;">
         <children>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" />
            <AnchorPane prefHeight="200.0" prefWidth="200.0" />
            <AnchorPane prefHeight="200.0" prefWidth="200.0" />
         </children>
      </HBox>
      <Button fx:id="add1" layoutX="351.0" layoutY="436.0" mnemonicParsing="false" onAction="#Added" prefHeight="22.0" prefWidth="91.0" text="Add">
         <font>
            <Font name="Cambria" size="18.0" />
         </font>
      </Button>
      <TextField fx:id="textfield" alignment="TOP_LEFT" layoutX="103.0" layoutY="179.0" prefHeight="71.0" prefWidth="278.0" promptText="Goal" />
      <DatePicker fx:id="date1" layoutX="103.0" layoutY="302.0" onAction="#pickdate" prefHeight="47.0" prefWidth="278.0" />
      <Button fx:id="back1" layoutX="43.0" layoutY="441.0" mnemonicParsing="false" onAction="#back" prefHeight="34.0" prefWidth="91.0" text="Back">
         <font>
            <Font name="Cambria" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
